{
  "name": "trae-monorepo-git-tag",
  "displayName": "Trae Monorepo Git Tag",
  "version": "0.0.1",
  "description": "A Trae IDE plugin for standardizing git tag commands in monorepo projects",
  "publisher": "voosil",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "trae-monorepo-git-tag.createTag",
        "title": "Create Standardized Git Tag"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "lint": "eslint src --ext ts",
    "publish:ovsx": "npx ovsx publish -p ${OVSX_PAT}"
  },
  "keywords": [
    "trae",
    "plugin",
    "git",
    "tag",
    "monorepo"
  ],
  "author": "Voosil",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/voosil/trae-monorepo-git-tag"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/vscode": "^1.100.0",
    "@vscode/vsce": "^3.6.0",
    "generator-code": "^1.11.11",
    "typescript": "^5.9.2",
    "yo": "^5.1.0"
  }
}
